{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },

    "globals": {
        "React": true
    },

    "parserOptions": {
        "ecmaFeatures": {
          "jsx": true
        }
    },


    "plugins": [
        "react",
        "babel"
    ],

    "extends": ["airbnb", "eslint:recommended", "plugin:react/recommended"]
}
